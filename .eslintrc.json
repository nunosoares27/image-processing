{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "semi": "off",
        "no-undef": "off",
        "comma-dangle": "off",
        "curly": "off",
        "space-before-function-paren": "off",
        "array-callback-return": "off"
    }
}